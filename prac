ex-8

echo "line 1" >>file.txt
git add file.txt
git commit -m "commit 1"
echo "line 2" >>file.txt
git add file.txt
git commit -m "commit 2"
echo "line 3" >>file.txt
git add file.txt
git commit -m "commit 3"
git branch source-branch
git checkout source-branch
echo "feature A" >>file.txt
git add file.txt
git commit -m "feature A commit"
echo "feature B" >>file.txt
git add file.txt
git commit -m "feature B commit"
echo "feature C" >>file.txt
git add file.txt
git commit -m "feature C commit"
git checkout master 
git log --oneline source-branch
git cherry-pick [feature A] ^..[feature C]

